{
  "name": "Robotic Control",
  "slug": "robotic-control",
  "version": "2.0.0",
  "description": "OpenClaw integration for robotic arm and gripper manipulation",
  "author": "Aly-Joseph",
  "license": "MIT",
  "keywords": ["robotics", "openclaw", "arm", "gripper", "manipulation"],
  "category": "Hardware",
  "status": "active",
  "homepage": "https://github.com/Aly-Joseph/J.A.R.V.I.S",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aly-Joseph/J.A.R.V.I.S.git"
  },
  "dependencies": [
    "openclaw",
    "pyserial",
    "numpy"
  ],
  "capabilities": [
    "robotic-arm-control",
    "gripper-manipulation",
    "precise-positioning",
    "force-sensing",
    "collision-detection",
    "sequence-execution"
  ],
  "requiredFiles": ["openclaw_control.py"],
  "interfaces": {
    "init_claw": {
      "description": "Initialize robotic claw",
      "returns": "OpenClawInterface"
    },
    "grab": {
      "description": "Close gripper to grab object",
      "parameters": {"force": "float", "duration": "float"},
      "returns": "bool"
    },
    "release": {
      "description": "Open gripper to release object",
      "parameters": {"speed": "float"},
      "returns": "bool"
    },
    "move_to": {
      "description": "Move to absolute position",
      "parameters": {"x": "float", "y": "float", "z": "float", "speed": "float"},
      "returns": "bool"
    },
    "rotate": {
      "description": "Rotate gripper",
      "parameters": {"angle": "float", "axis": "str", "speed": "float"},
      "returns": "bool"
    }
  }
}
